{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:full": "npm run dev & sleep 3 && npm run services:start",
    "dev:https": "next dev --experimental-https",
    "services:start": "curl -s \"http://localhost:3000/api/health?action=init\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "bot:setup": "node scripts/bot-setup.js",
    "bot:start": "curl -X POST http://localhost:3000/api/bot/start",
    "bot:stop": "curl -X DELETE http://localhost:3000/api/bot/start",
    "bot:status": "curl -X GET http://localhost:3000/api/bot/start",
    "bot:webhook": "curl -X GET http://localhost:3000/api/bot/webhook",
    "bot:logs": "curl -X GET 'http://localhost:3000/api/bot/logs?recent=true&limit=20'",
    "bot:analytics": "curl -X GET 'http://localhost:3000/api/bot/logs?analytics=true&days=7'",
    "bot:cleanup": "curl -X DELETE 'http://localhost:3000/api/bot/logs?days=30'",
    "matching:status": "curl -X GET 'http://localhost:3000/api/matching'",
    "matching:run": "curl -X POST 'http://localhost:3000/api/matching?action=run'",
    "matching:stats": "curl -X GET 'http://localhost:3000/api/matching?action=stats'",
    "matching:config": "curl -X GET 'http://localhost:3000/api/matching?action=config'",
    "matching:mock-users": "curl -X POST 'http://localhost:3000/api/matching?action=create-mock-users' -H 'Content-Type: application/json' -d '{\"count\": 15}'",
    "matching:cleanup": "curl -X POST 'http://localhost:3000/api/matching?action=cleanup'",
    "scheduler:start": "curl -X POST 'http://localhost:3000/api/matching?action=start-scheduler'",
    "scheduler:stop": "curl -X POST 'http://localhost:3000/api/matching?action=stop-scheduler'",
    "scheduler:status": "curl -X GET 'http://localhost:3000/api/matching?action=scheduler'",
    "scheduler:trigger": "curl -X POST 'http://localhost:3000/api/matching?action=trigger-manual'",
    "health:check": "curl -X GET 'http://localhost:3000/api/health'",
    "health:init": "curl -X GET 'http://localhost:3000/api/health?action=init'",
    "db:seed": "npx tsx scripts/seed-database.ts",
    "db:seed:api": "curl -X POST http://localhost:3000/api/database/seed"
  },
  "dependencies": {
    "@telegram-apps/sdk-react": "^2.0.5",
    "@telegram-apps/telegram-ui": "^2.1.5",
    "@types/node-cron": "^3.0.11",
    "autoprefixer": "^10.4.20",
    "eruda": "^3.0.1",
    "googleapis": "^148.0.0",
    "mongodb": "^6.11.0",
    "mongodb-memory-server": "^10.1.2",
    "mongoose": "^8.8.4",
    "next": "14.2.4",
    "next-intl": "^3.17.6",
    "node-cron": "^4.1.1",
    "normalize.css": "^8.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.54.0",
    "react-swipeable": "^7.0.1",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@types/node": "^20.19.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
